{
  "name": "signalk-fusion-stereo",
  "version": "1.14.1",
  "description": "Plugin that controls a Fusion stereo",
  "main": "dist/index.js",
  "scripts": {
    "test": "$NODE $npm_package_main",
    "build_ios": "webpack ios.js -o ios_fusion.js",
    "lint": "eslint --fix",
    "prettier": "prettier --write ./src/*",
    "ci-lint": "eslint && prettier --check .",
    "format": "npm run prettier && npm run lint",
    "build": "tsc -b --pretty false",
    "watch": "tsc --watch --pretty false",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "signalk-node-server-plugin"
  ],
  "author": "scott@scottbender.net",
  "license": "ISC",
  "dependencies": {
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "util": "^0.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sbender9/signalk-fusion-stereo"
  },
  "signalk-plugin-enabled-by-default": true,
  "devDependencies": {
    "@signalk/server-api": "^2.7.1",
    "@tsconfig/node20": "^20.1.6",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier-standard": "^16.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
